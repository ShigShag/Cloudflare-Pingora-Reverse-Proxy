# Host and Port for Pingora to listen on
PROXY_HOST=0.0.0.0
PROXY_PORT=6188

# Directory containing the TLS certificate and key
CERT_DIR=./certificate

# Certificate and key filenames (defaults: server.crt / server.key)
CERT_FILE=cert.pem
KEY_FILE=key.pem

# Path to the YAML routing configuration
CONFIG_PATH=./config/config.yaml

# Logging Level (info, debug, warn, error)
RUST_LOG=info
